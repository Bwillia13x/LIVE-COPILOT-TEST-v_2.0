# Production Environment Variables
# This file contains production-specific configurations

# Application Configuration
VITE_APP_ENV=production
VITE_APP_NAME="Voice Notes Pro"
VITE_APP_VERSION=1.0.0

# API Configuration
# Note: Set your production Gemini API key in your hosting platform's environment variables
# VITE_GEMINI_API_KEY=your_production_api_key_here

# Performance Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_CONSOLE_LOGS=false
VITE_ENABLE_TEST_FEATURES=false

# Security Configuration
VITE_SECURE_MODE=true
VITE_CORS_ENABLED=true

# CDN and Assets
VITE_CDN_URL=https://cdn.jsdelivr.net
VITE_ASSETS_URL=/assets

# Production Optimizations
VITE_BUNDLE_ANALYZER=false
VITE_MINIFY=true
VITE_TREE_SHAKING=true

# Monitoring and Logging
VITE_LOG_LEVEL=error
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# Domain Configuration
VITE_DOMAIN=voicenotes.app
VITE_API_BASE_URL=https://api.voicenotes.app
